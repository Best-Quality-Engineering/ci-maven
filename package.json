{
  "name": "ci-maven-action",
  "version": "1.0.1",
  "description": "Executes Maven goals with CI friendly properties",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib/* && rm -rf dist/*",
    "compile": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Best-Quality-Engineering/ci-maven.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "TypeScript"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Best-Quality-Engineering/ci-maven/issues"
  },
  "homepage": "https://github.com/Best-Quality-Engineering/ci-maven",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^16.18.21",
    "@typescript-eslint/parser": "^5.56.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.36.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
